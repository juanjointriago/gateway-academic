{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "$(GOOGLE_SERVICES_JSON)",
        "GOOGLE_SERVICE_INFO_PLIST": "$(GOOGLE_SERVICE_INFO_PLIST)"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "$(GOOGLE_SERVICES_JSON)",
        "GOOGLE_SERVICE_INFO_PLIST": "$(GOOGLE_SERVICE_INFO_PLIST)"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "$(GOOGLE_SERVICES_JSON)",
        "GOOGLE_SERVICE_INFO_PLIST": "$(GOOGLE_SERVICE_INFO_PLIST)"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
